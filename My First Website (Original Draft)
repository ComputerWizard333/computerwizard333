 <script>
     var aAudio = new Audio('https:\/\/files.freemusicarchive.org\/storage-freemusicarchive-org\/music\/no_curator\/Gassyoh\/Cow_ep\/Gassyoh_-_03_-_Techno_Mahal.mp3');
     var bAudio = new Audio('b.mp3');
     function myAudioFunction(letter) {
         if(letter == 'a') {
             aAudio.play();
         } else if(letter == 'b') {
             bAudio.play();
         }
     }
 </script>
